#include<stdio.h>


int main()
{
    int a=5;
    int *p; //pointer declaration which points int datatype
    p=&a; // & for address

    printf("The actual value of a is:%d\n",a);
    printf("The address of actual value of a is:%p\n",&a);
    printf("The address of actual value of a is:%p\n",p); // Same result, which is obtained at &p
    printf("Dereferencing the using pointer actual value of a is:%d\n",*p); // Prints actual value using pointer called dereferencing

}
